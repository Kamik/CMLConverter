Функция КодыЕдиницИзмерения()
	
	Результат = Новый Соответствие;
	
	Результат.Вставить("шт",		"796");
	Результат.Вставить("упак",		"778");
	Результат.Вставить("кг",		"166");
	Результат.Вставить("ящ",		"812");
	Результат.Вставить("дес",		"_01");//нет в ОКЕИ
	//... из макета надо
	
	Возврат Результат;
	
КонецФункции